  "bugs": {
    "url": "https://github.com/npm/nsq-registry-event-client/issues"
  },
  "homepage": "https://github.com/npm/nsq-registry-event-client#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "nyc": "^10.2.2-candidate.4",
    "standard": "^10.0.2",
    "uuid": "^3.0.1"
  },
  "dependencies": {
    "aproba": "^1.1.1",
    "nsqjs": "^0.8.4"
  }
