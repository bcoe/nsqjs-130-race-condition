{
  "name": "@npm/nsq-registry-event-client",
  "version": "1.0.1",
  "description": "read/write registry events to nsq in a standardized format",
  "main": "./lib/client",
  "scripts": {
    "pretest": "standard",
    "test:sleep": "nyc mocha --timeout=5000 ./test/sleepy-client.js",
    "test:nosleep": "nyc mocha --timeout=5000 --recursive ./test/no-sleep-client.js",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/npm/nsq-registry-event-client.git"
  },
